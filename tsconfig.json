{
  "files": [
    "src/main.ts"
  ],
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": "src",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "rootDir": "src",
    "outDir": "transpiled/app",
    "declaration": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "lib": ["es6", "dom"]
  }
}
